{
  "name": "entity-wiki",
  "homepage": "https://entity-toolkit.github.io",
  "bugs": {
    "url": "https://github.com/entity-toolkit/wiki/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/entity-toolkit/wiki.git"
  },
  "directories": {
    "doc": "docs"
  },
  "scripts": {
    "sass:watch": "sass extra_sass/style.scss docs/css/style.css --watch",
    "sass:build": "sass extra_sass/style.scss docs/css/style.css",
    "mkdocs:serve": "mkdocs serve",
    "mkdocs:build": "mkdocs build",
    "dev": "concurrently \"npm run sass:watch\" \"npm run mkdocs:serve\""
  },
  "dependencies": {
    "@fsouza/prettierd": "^0.26.1",
    "emmet-ls": "^0.4.2",
    "vscode-langservers-extracted": "^4.10.0"
  },
  "devDependencies": {
    "concurrently": "^9.2.0",
    "sass": "^1.89.2"
  }
}
